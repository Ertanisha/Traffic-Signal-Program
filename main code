  digitalWrite(7,LOW);
  digitalWrite(8,LOW);
  delay(1000);
}

Serial.println(S4);

  if (S4 <= 200) {
  
 digitalWrite(11,LOW);
 digitalWrite(12,HIGH);  // CASE 4 ON.



