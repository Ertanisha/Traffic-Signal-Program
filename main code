  digitalWrite(6,LOW);
  digitalWrite(7,LOW);
  digitalWrite(8,LOW);
  delay(1000);
}

Serial.println(S3);

  if (S3 <= 200) {
  
 digitalWrite(10,LOW);
 digitalWrite(11,HIGH);  // CASE 3 ON.

 

  digitalWrite(2,HIGH);
  digitalWrite(3,HIGH);
  digitalWrite(4,HIGH);
  digitalWrite(5,LOW);
  digitalWrite(6,LOW);
